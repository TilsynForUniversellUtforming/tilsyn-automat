<!DOCTYPE html>
<html lang="no">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Egenkontroll</title>

    <environment include="Development">
        <link href="~/css/foundation.css" rel="stylesheet" />
    </environment>

    <environment exclude="Development">
        <link href="~/css/foundation.min.css" rel="stylesheet" />
    </environment>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">
    <link href="https://use.typekit.net/iij2bts.css" rel="stylesheet">
</head>
<body>


    @{ await Html.RenderPartialAsync("Shared/_Done", new { });}
    @{ await Html.RenderPartialAsync("Shared/_Error", new { });}

    @RenderBody()

    <environment include="Development">
        <script src="~/js/jquery/jquery.js"></script>
        <script src="~/js/foundation.js"></script>
        <script src="~/js/jquery.tablesorter/js/jquery.tablesorter.js"></script>
        <script src="~/js/site.js"></script>
        <script src="~/js/funka/aria.toggle.js"></script>
        <script src="~/js/funka/tab.focus.js"></script>
        <script src="~/js/funka/clickable-element.js"></script>
    </environment>

    <environment exclude="Development">
        <script src="~/js/jquery/jquery.min.js"></script>
        <script src="~/js/foundation.min.js"></script>
        <script src="~/js/jquery.tablesorter/js/jquery.tablesorter.min.js"></script>
        <script src="~/js/site.min.js"></script>
        <script src="~/js/funka/aria.toggle.js"></script>
        <script src="~/js/funka/tab.focus.js"></script>
        <script src="~/js/funka/clickable-element.js"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>