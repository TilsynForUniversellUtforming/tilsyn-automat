@model Microsoft.AspNetCore.Http.IFormFile



<div class="fileUploadButtonsArea">
    <label for="@Html.IdForModel()">
        @Html.DisplayNameFor(m => m)
        @Html.ValidationMessageFor(m => m)
    </label>

    <input class="button border-button choose-file" asp-for="@Model" type="file" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
</div>



